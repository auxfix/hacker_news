  "scripts": {
    "start": "node tools/start",
    "build": "node tools/build",
    "build:serve": "npm run build && npm run serve",
    "analyze": "NODE_ENV=production webpack --config config/webpack.config.js --profile --json > webpack.stats.json",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src config test tools",
    "lint:styles:ci": "stylelint \"src/**/*.{sass,scss}\"",
    "lint:styles": "stylelint \"src/**/*.{ts,tsx}\" --config .stylelintrc_common && stylelint \"src/**/*.{sass,scss}\" --config .stylelintrc_sass",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:coverage": "jest --bail --coverage",
    "test:watch": "jest --watchAll --verbose",
    "test:e2e": "start-server-and-test serve http://localhost:3000 test:e2e:run",
    "test:e2e:run": "cypress run",
    "test:e2e:dev": "cypress open",
    "format": "prettier \"**/*.{ts,tsx}\" --write",
    "validate": "npm run lint && npm run lint:styles:ci && npm run test:coverage && npm run build && size-limit",
    "serve": "serve --listen 3000 build",
    "size": "npm run build && size-limit",
    "postinstall": "rm -rf node_modules/@types/react-native",
    "prepare": "husky install"
  },
  "prettier": "@gilbarbara/prettier-config",
  "lint-staged": {
    "*.ts?(x)": [
      "eslint",
      "stylelint --config .stylelintrc_common",
      "git add",
      "jest --findRelatedTests"
    ],
    "*.(js|jsx|json|yml|yaml|css|less|scss|ts|tsx|md|graphql|mdx)": [
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "size-limit": [
    {
      "path": "./build/static/js/*.js",
      "limit": "200 kb"
    }
  ]
}
